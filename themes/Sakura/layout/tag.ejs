<%
var img = '/images/tags.jpg';
var en = '我好像住你隔壁';
%>
    <!-- <div class="blank">
    </div> -->
    <div class="pattern-center-blank">
    </div>
    <div class="pattern-center ">
        <div class="pattern-attachment-img">
            <img src="https://cdn.jsdelivr.net/gh/drew233/cdn/20200303155335.webp" data-src="<%= __(img) %>" class="lazyload" onerror="imgError(this,3)" style="width: 100%; height: 100%; object-fit: cover; pointer-events: none;">
        </div>
        <header class="pattern-header ">
            <h1 class="entry-title">
                标签 |
                <%= __(page.tag) %>
            </h1>
            <span class="cat-des">
              <i class="fa fa-venus-double" aria-hidden="true" style="color: lightcoral;"></i>
            <p><%= __(en) %></p>
            </span>
        </header>
    </div>

    <div id="content" class="site-content">
        <div id="primary" class="content-area kuangjia">
            <main id="main" class="site-main" role="main">
                <%- partial('_partial/category-archive', {pagination: 2, index: true}) %>
                    <div class="clearer">
                    </div>
            </main>
            <% if (page.total > 1 && page.next_link){ %>
                <div id="pagination"><a href="<%- url_for(page.next_link) %>" class="">Previous</a></div>
                <% } %>
        </div>
    </div>